[ PSEUDOKOD ]
mod(a[in], b[in], p[inout])
    p := 0
    if b != 0:
        b_copy = b;
        while a >= b:
            b *= 2;
        end

        repeat:
            b /= 2;
            IF b <= a
                a -= b;
        until a < b_copy
    p := a
    endif
return

