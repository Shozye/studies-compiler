IF_BODY: b_copy := b;
IF a < b GOTO E_MOD_WHILE2_BODY
E_MOD_WHILE1_BODY: b *= 2
IF a >= b GOTO E_MOD_WHILE1_BODY

E_MOD_WHILE2_BODY : b /= 2
IF b > a GOTO E_MOD_WHILE2_BODY
a -= b;
IF a >= b_copy GOTO E_MOD_WHILE2_BODY

p := a
RETURN [E_ENDWHILE ]


