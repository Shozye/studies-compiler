GET a
GET b
a_copy = a
p := 0
if b == 0:
    while a_copy >= b
        temp := 1 [ E_WHILE_BODY ]
        b_copy := b
        while a_copy >= b_copy
            temp *= 2 [ E_INNER_WHILE_BODY ]
            b_copy *= 2
        end
        b_copy /= 2 [ E_INNER_ENDWHILE ]
        temp /= 2
        p := p + temp
        a_copy := a_copy - b_copy
    end
PUT p [E_ENDWHILE ]

