GET a
GET b
a_copy := a
p := 0
IF b == 0 GOTO E_ENDWHILE
IF a_copy < b GOTO E_ENDWHILE
temp := 1 [ E_WHILE_BODY ]
b_copy := b
IF b_copy - a_copy > 0 GOTO E_INNER_ENDWHILE
temp *= 2 [ E_INNER_WHILE_BODY ]
b_copy *= 2
IF b_copy - a_copy = 0 GOTO E_INNER_WHILE_BODY
b_copy /= 2 [ E_INNER_ENDWHILE ]
temp /= 2
p := p + temp
a_copy := a_copy - b_copy
IF a_copy >= b GOTO E_WHILE_BODY
PUT p [E_ENDWHILE ]

