[ Program Reads and multiplies two numbers ]
PROCEDURE decrement(a) IS
BEGIN
    a := a - 1;
END

PROCEDURE mul(a, b, p) IS
VAR a_copy
BEGIN
    a_copy := a;
    p := 0;
    WHILE a_copy != 0
    DO
        p := p + b;
        decrement(a_copy);
    ENDWHILE
END

PROGRAM IS
VAR a, b, p
BEGIN
    READ a;
    READ b;
    mul(a,b,p);
    WRITE p;
END
